@{
  Layout = "_Layout";
}

<div class='main-container'>
<div class='form-container'>
<h1>Order Details: </h1>

<h2>@Model["order"].Name</h2>
<h2>@Model["order"].Description</h2>
<h2>@Model["order"].Price</h2>
<h2>@Model["order"].DeliveryDate</h2>
<h3>Category: @Model["vendor"].Name</h3>

<a href='/vendors/@Model["vendor"].Id/orders/new'>Add another order</a>
<a href='/vendors'>View all vendors</a>
</div >
</div>